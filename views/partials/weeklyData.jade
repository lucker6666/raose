div(class='alert alert-success') App相关数据

div(class='row')
    
    div(class='col-md-12')  
        div(class='panel panel-success')
            div(class='panel-heading') 日新增用户
            div(class='panel-body') 
                div(id='app-install',style='height:325px;')
                span(class='no') {{app.thisWeekInstallAvg}} 
                周环比增长 {{app.weeklyInstallSum}}

    div(class='col-md-2')  
        div(class='panel panel-danger')
            div(class='panel-heading') APP当前排名
            div(class='panel-body center') 
                span(class='no vivid no-lg') {{appRank.no}} 
                br
                span(class='gray') 最后更新于
                br
                span(class='gray') {{appRank.time}}


    div(class='col-md-5')  
        div(class='panel panel-success')
            div(class='panel-heading') 日活跃用户
            div(class='panel-body') 
                div(id='app-active',style='height:100px;')
                span(class='no no-sm') {{app.thisWeekActiveAvg}} 
                周环比增长 {{app.weeklyActiveSum}}

    div(class='col-md-5')  
        div(class='panel panel-success')
            div(class='panel-heading') 日启动用户
            div(class='panel-body') 
                div(id='app-launch',style='height:100px;')
                span(class='no-sm') {{app.thisWeekLaunchesAvg}} 
                周环比增长 {{app.weeklyLaunchesSum}}
